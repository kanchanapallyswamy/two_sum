class Main{
    public static void main(String[] args) {
        int a[]={2,5,6,11,10};
        int len=a.length;
        int sum=21;
        int l=0,r=len-1;
        while(l<r){
            if(a[l]+a[r]==sum){
                System.out.println(a[l]+" "+a[r]);
                break;
            }
            else if(a[l]+a[r]<sum){
                l++;
            }
            else{
                r--;
            }
        }
    }
}
